# This file serves as a template for development settings
# Copy this to .env.local and fill in your actual values

# Use local Supabase instance when true
NEXT_PUBLIC_USE_LOCAL_SUPABASE=true

# Local Supabase settings
# Get these from running: npx supabase status
NEXT_PUBLIC_SUPABASE_LOCAL_URL=http://127.0.0.1:54321
NEXT_PUBLIC_SUPABASE_LOCAL_ANON_KEY=your_local_anon_key_here

# Production Supabase settings (only needed if switching to production)
NEXT_PUBLIC_SUPABASE_URL=your_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here 